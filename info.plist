				<string>function run(argv) {
  // Sanity Checks
  if (!Array.isArray(argv) &amp;&amp; !argv.length) return argv;
  if (typeof argv[0] !== 'string') return argv;

  var device = argv[0];
  ObjC.import('stdlib');
  Application('com.runningwithcrayons.Alfred').setConfiguration('LAST_INPUT', {
    toValue: device,
    inWorkflow: $.getenv('alfred_workflow_bundleid'),
    exportable: true
  });

  return device;
}</string>
